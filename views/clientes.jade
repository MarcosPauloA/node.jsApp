doctype html
html(lang="pt-br")
    head
        title Clientes

    body
        h1 Lista de clientes
        table(border=1)
            tr
                th Nome 
                th Sobrenome
                th Email
                th Idade

            each item in listaClientes
                tr
                    td= item.nome 
                    td= item.sobrenome 
                    td= item.email
                    td= item.idade
